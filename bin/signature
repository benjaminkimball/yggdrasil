#!/usr/bin/env bash

RED='\033[0;31m'
YELLOW='\033[0;33m'
GREEN='\033[0;32m'
CYAN='\033[0;36m'
BLUE='\033[0;34m'
MAGENTA='\033[0;35m'
RESET='\033[0m'

echo
echo
printf "${RED}▄▀▀▄ ▀▀▄  ▄▀▀▀▀▄    ▄▀▀▀▀▄   ▄▀▀█▄▄   ▄▀▀▄▀▀▀▄  ▄▀▀█▄   ▄▀▀▀▀▄  ▄▀▀█▀▄   ▄▀▀▀▀▄${RESET}\n"
printf "${YELLOW}█   ▀▄ ▄▀ █         █        █ ▄▀   █ █   █   █ ▐ ▄▀ ▀▄ █ █   ▐ █   █  █ █    █${RESET}\n"
printf "${GREEN}▐     █   █    ▀▄▄  █    ▀▄▄ ▐ █    █ ▐  █▀▀█▀    █▄▄▄█    ▀▄   ▐   █  ▐ ▐    █${RESET}\n"
printf "${CYAN}      █   █     █ █ █     █ █  █    █  ▄▀    █   ▄▀   █ ▀▄   █      █        █${RESET}\n"
printf "${BLUE}    ▄▀    ▐▀▄▄▄▄▀ ▐ ▐▀▄▄▄▄▀ ▐ ▄▀▄▄▄▄▀ █     █   █   ▄▀   █▀▀▀    ▄▀▀▀▀▀▄   ▄▀▄▄▄▄▄▄▀${RESET}\n"
printf "${MAGENTA}    █     ▐         ▐        █     ▐  ▐     ▐   ▐   ▐    ▐      █       █  █${RESET}\n"
printf "${MAGENTA}    ▐                        ▐                                  ▐       ▐  ▐${RESET}\n"
echo
