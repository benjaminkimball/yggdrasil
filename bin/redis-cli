#!/usr/bin/env bash

BLUE='\033[0;34m'
RESET='\033[0m'

CONTAINER=`docker ps | grep -Eo '\w+_redis_\d+'`

echo
echo
printf "${BLUE}$(cat bin/signature.txt)${RESET}"
echo
echo
echo "============================================================"
echo "> Connecting to $CONTAINER..."
echo

docker exec -it $CONTAINER redis-cli
